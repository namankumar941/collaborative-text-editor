<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head') %>
    <title>Home</title>
</head>
<body>
    <%- include('./partials/nav') %>
    <div class="container mt-3" >
        <form action="/myFiles/editName/<%= docId %>" method = "post" >
            <div class="mb-3">
                <label for="name" class="form-label"></label>
                <input type="text"
                placeholder="Name" 
                required
                class="form-control" 
                id="name" 
                name="name" 
                aria-describedby="name">
            </div>

            <div class="mb-3">
                <button class="btn btn-primary">Submit</button>
             </div>

        </form>
    </div>
    <%- include('./partials/scripts') %>
</body>
</html>